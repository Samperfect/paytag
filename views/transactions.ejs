<!DOCTYPE html>
<html lang="en">
  <!-- Mirrored from Paytag.bragherstudio.com/view4/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Thu, 30 Sep 2021 23:26:29 GMT -->
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, viewport-fit=cover"
    />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta
      name="apple-mobile-web-app-status-bar-style"
      content="black-translucent"
    />
    <meta name="theme-color" content="#000000" />
    <title>Paytag | Dashboard</title>
    <meta name="description" content="Paytag HTML Mobile Template" />
    <meta
      name="keywords"
      content="paytag, borderless, cardless, payment, buycoins, patricia, paypal, tag, pay, payoneer"
    />
    <link
      rel="icon"
      type="image/png"
      href="assets/img/favicon.png"
      sizes="32x32"
    />
    <!-- <link
      rel="apple-touch-icon"
      sizes="180x180"
      href="assets/img/icon/192x192.png"
    /> -->
    <link rel="stylesheet" href="assets/css/style.css" />
    <link rel="manifest" href="__manifest.json" />
  </head>

  <body>
    <!-- loader -->
    <div id="loader">
      <img src="assets/img/loading-icon.png" alt="icon" class="loading-icon" />
    </div>
    <!-- * loader -->

    <!-- App Header -->
    <div class="appHeader">
      <div class="left">
        <a href="#" class="headerButton goBack">
          <ion-icon name="chevron-back-outline"></ion-icon>
        </a>
      </div>
      <div class="pageTitle">Transactions</div>
      <div class="right">
        <!-- <a href="app-notifications.html" class="headerButton">
          <ion-icon class="icon" name="notifications-outline"></ion-icon>
          <span class="badge badge-danger">4</span>
        </a> -->
      </div>
    </div>
    <!-- * App Header -->

    <!-- App Capsule -->
    <div id="appCapsule">
      <!-- Send Action Sheet -->
      <div
        class="modal fade action-sheet"
        id="reedemActionSheet"
        tabindex="-1"
        role="dialog"
      >
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title">Reedem Paytag</h5>
            </div>
            <div class="modal-body">
              <div class="action-sheet-content">
                <p class="text-muted" style="line-height: initial">
                  Reedem the funds in a Paytag into your account.
                </p>
                <p class="text-muted" style="line-height: initial">
                  You only need a pin if the Paytag has one!
                </p>

                <form action="" autocomplete="off">
                  <div class="form-group basic">
                    <label class="label">Enter Paytag</label>
                    <div class="input-group mb-2">
                      <input type="text" style="display: none" />
                      <span class="input-group-text" id="basic-addonb1"
                        ><ion-icon name="pricetag-outline"></ion-icon
                      ></span>
                      <input
                        type="text"
                        required
                        class="form-control"
                        placeholder="Enter your Paytag"
                        name="paytag"
                        minlength="5"
                        maxlength="5"
                        autocomplete="off"
                        autocapitalize="characters"
                        style="text-transform: uppercase"
                      />
                    </div>
                  </div>
                  <div class="form-group basic">
                    <label class="label">Paytag Pin (optional)</label>

                    <div class="input-group mb-2">
                      <input type="password" style="display: none" />
                      <span class="input-group-text" id="basic-addonb1"
                        ><ion-icon name="key-outline"></ion-icon
                      ></span>

                      <input
                        type="password"
                        autocomplete="off"
                        name="pin"
                        class="form-control"
                        placeholder="only if the Paytag requires a pin"
                        minlength="4"
                        maxlength="4"
                        value=""
                      />
                    </div>
                  </div>
                  <!-- data-bs-dismiss="modal" -->
                  <div class="form-group basic">
                    <input
                      type="submit"
                      class="btn btn-primary btn-block btn-lg"
                      value="Reedem"
                    />
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- * Send Action Sheet -->

      <!-- Transactions -->
      <div class="section mt-4">
        <!-- <div class="section-heading"> -->
        <!-- <h2 class="title">My Transactions</h2> -->
        <!-- <a href="/user/transactions" class="link">View All</a> -->
        <!-- </div> -->
        <div class="transactions">
          <% if (transactions.length > 0){ %> <% transactions.forEach(
          transaction => { %>

          <!-- item -->
          <a
            href="/user/transactions/<%= transaction.transactionId._id %>"
            class="item"
          >
            <div style="font-size: 12px" class="detail">
              <img
                src="<%= transaction.transactionId.media %>"
                alt="img"
                class="image-block imaged w48"
              />
              <div>
                <strong><%= transaction.transactionId.description %></strong>
                <p>
                  <%= new
                  Date(transaction.transactionId.createdAt).toDateString() %>
                </p>
              </div>
            </div>

            <div class="right">
              <div
                style="font-size: 12px"
                class="
                  price
                  text-<%=
                  transaction.transactionId.sign
                  ==
                  '-'?
                  'danger':
                  transaction.transactionId.sign
                  ==
                  '+'
                  ?
                  'success'
                  :
                  'primary'
                  %>
                "
              >
                <%= transaction.transactionId.sign %> <%= user.currency %> <%=
                transaction.transactionId.amount.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g,
                ",") %>
              </div>
            </div>
          </a>
          <!-- * item -->

          <% } )%> <% } else { %>

          <a class="item">
            <div class="detail">There are no transactions here</div>
          </a>

          <% } %>
        </div>
      </div>
      <!-- * Transactions -->

      <li style="display: none">
        <a
          href="#"
          class="item"
          data-bs-toggle="modal"
          id="smodal"
          data-bs-target="#sModal"
        >
          <div class="in">
            <div>Success</div>
          </div>
        </a>
      </li>
      <li style="display: none">
        <a
          href="#"
          class="item"
          id="fmodal"
          data-bs-toggle="modal"
          data-bs-target="#fModal"
        >
          <div class="in">
            <div>Failure</div>
          </div>
        </a>
      </li>

      <!-- DialogIconedSuccess -->
      <div
        class="modal fade dialogbox"
        id="fModal"
        data-bs-backdrop="static"
        tabindex="-1"
        role="dialog"
      >
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-icon text-danger">
              <ion-icon name="close-circle"></ion-icon>
            </div>
            <div class="modal-header">
              <h5 class="modal-title">Failed</h5>
            </div>
            <div class="modal-body fmsg">Something went wrong!</div>
            <div class="modal-footer">
              <div class="btn-inline">
                <a href="#" class="btn" data-bs-dismiss="modal">CLOSE</a>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- * DialogIconedSuccess -->

      <!-- Dialog Form -->
      <div
        class="modal fade dialogbox"
        id="sModal"
        data-bs-backdrop="static"
        tabindex="-1"
        role="dialog"
      >
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-icon text-success">
              <ion-icon name="checkmark-circle"></ion-icon>
            </div>
            <div class="modal-header">
              <h5 class="modal-title smsg">Success</h5>
            </div>

            <div class="modal-body text-start mb-2">
              <div class="form-group basic">
                <div class="input-wrapper">
                  <label class="label" for="text1">Your Paytag</label>
                  <input type="text" class="form-control" id="newpaytag" />
                  <i class="clear-input">
                    <ion-icon name="close-circle"></ion-icon>
                  </i>
                </div>
              </div>
            </div>
            <div class="modal-footer">
              <div class="btn-inline">
                <button
                  type="button"
                  class="btn btn-text-secondary"
                  data-bs-dismiss="modal"
                >
                  COPY
                </button>
                <button
                  type="button"
                  class="btn btn-text-primary"
                  data-bs-dismiss="modal"
                >
                  CLOSE
                </button>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- * Dialog Form -->

      <!-- app footer -->
      <div class="appFooter">
        <div class="footer-title">
          Copyright Â© Paytag 2021. All Rights Reserved.
        </div>
      </div>
      <!-- * app footer -->
    </div>
    <!-- * App Capsule -->

    <!-- App Bottom Menu -->
    <%- include('./partials/menu') %>

    <!-- * App Bottom Menu -->

    <!-- App Sidebar -->
    <%- include('./partials/sidebar') %>
    <!-- * App Sidebar -->

    <!-- PWA SECTION -->
    <%- include('./partials/pwa') %>
    <!-- PWA SECTION END -->

    <!-- ========= JS Files =========  -->
    <%- include('./partials/footer') %>
  </body>

  <!-- Mirrored from Paytag.bragherstudio.com/view4/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Thu, 30 Sep 2021 23:26:40 GMT -->
</html>
