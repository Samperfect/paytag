<% if(success_msg != '') { %>

<div
  class="alert alert-imaged alert-success alert-dismissible fade show"
  role="alert"
>
  <!-- <div class="icon-wrap">
                            <ion-icon name="card-outline"></ion-icon>
                        </div> -->
  <div><%= success_msg %></div>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>

<% } %> <% if(error_msg != '') { %>
<div class="alert alert-danger alert-dismissible fade show" role="alert">
  <div><%= error_msg %></div>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if(error != '') { %>
<div class="alert alert-danger alert-dismissible fade show" role="alert">
  <div><%= error %></div>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
